{
  "name": "node_type_orm",
  "version": "0.0.1",
  "description": "Basic concepts",
  "scripts": {
    "start": "tsc && node dist/server.js",
    "start:dev": "tsc && concurrently \"tsc -w\" \"nodemon dist/server.js\"",
    "start:test": "ts-node-dev ./src/server.ts",
    "start:prod": "export NODE_ENV=production && npm start",
    "typeorm": "typeorm-ts-node-esm:sync -d ./src/config/data.source.ts",
    "m:gen": "npm run typeorm migration:generate",
    "m:run": "npm run typeorm migration:run",
    "m:revert": "npm run typeorm migration:revert"
  },
  "author": "Alejandro Urbina",
  "license": "MIT",
  "dependencies": {
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.1",
    "morgan": "^1.10.0",
    "mysql2": "^2.3.3",
    "nvm": "^0.0.4",
    "reflect-metadata": "^0.1.10",
    "ts-node-dev": "^2.0.0",
    "typeorm": "^0.3.0",
    "typeorm-naming-strategies": "^4.1.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.11.9",
    "concurrently": "^7.4.0",
    "nodemon": "^2.0.20",
    "typescript": "^4.8.4"
  }
}